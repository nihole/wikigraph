digraph G {
	node [color=blue fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_nw_1"]
	"--1--" [label="--1--
Alice was at the
theatre yesterday
at 7 p.m."]
	node [color=red fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_pw_1"]
	"++1++" [label="++1++
Alice was not at
the theatre yesterday
at 7 p.m."]
	node [color=blue fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_nw_3"]
	"--3--" [label="--3--
Bob saw her at
the theatre yesterday
at 7 p.m."]
	node [color=blue fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_nw_4"]
	"--4--" [label="--4--
Mike saw her at
the theatre yesterday
at 7 p.m."]
	node [color=red fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_pw_10"]
	"++10++" [label="++10++
Bob is lying"]
	node [color=red fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_pw_11"]
	"++11++" [label="++11++
Mike was mistaken
and mistake another
girl for Alice"]
	node [color=blue fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_nw_10"]
	"--10--" [label="--10--
Bob is telling
the truth"]
	node [color=blue fontcolor=black href="https://github.com/nihole/wg_examples/wiki/examples_nw_11"]
	"--11--" [label="--11--
"]
	"--1--" -> "++1++" [color=red dir=both]
	"++1++" -> "--3--"
	"++1++" -> "--4--"
	"--3--" -> "++10++"
	"--4--" -> "++11++"
	"++10++" -> "--10--" [color=red dir=both]
	"++11++" -> "--11--" [color=red dir=both]
}
